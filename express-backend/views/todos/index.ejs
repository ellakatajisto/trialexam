All TodDo's from the database
<form action="/delete/ToDos" method="post" onclick="return confirm('delete all ToDo?')" type="submit" name="type"
    value="Delete All ToDos"></form>



<% todos.forEach(todo => { %>
<a href="<%= `/todos/${todo._id}` %>">
    <%= todo.title %>
</a>
<%= todo.done %>

<a href="<%=`/todos/${todo._id}/update`%>">
    Edit
</a>
<br>
<a href="<%= `todos/${todo._id}/delete?_method=DELETE` %>" onclick="return confirm('Delete this ToDo?')">Delete</a>
<% }); %>
<br>
<br>
<a href="<%= `new/` %>"> New task</a>
